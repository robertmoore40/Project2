<div class="container">
  <div class="row">
    <div class="col-s12">
      <h1 id="search-title" class="center">Movie Search</h1>
    </div>
  </div>


  <div class="row">
    <form class="col s12 m12 l12">
      <div class="row">
        <form>
          <div class="input-field col s3 offset-s4">
            <input placeholder="e.g. The Matrix" id="movieName" type="text">
            <label for="first_name"></label>
          </div>
          <div class="input-field col s5">
            <a type="submit" id="search-movie" class="waves-effect waves-light btn"><i
                class="material-icons right">library_add</i>Search Movies</a>
          </div>
        </form>
      </div>
    </form>
  </div>

</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/searchMovie.js"></script>